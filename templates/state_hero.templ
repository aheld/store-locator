package templates

templ StateHero(state string) {
  <h2>Market Hero</h2>
  <div class="hero min-h-screen bg-base-200">
  <div class="hero-content flex-col lg:flex-row-reverse">
    <div>
      <h1 class="text-5xl font-bold">{ state } Farmers Markets</h1>
      <p class="py-6">Select a market from the right --></p>
    </div>
  </div>
  </div>
}
